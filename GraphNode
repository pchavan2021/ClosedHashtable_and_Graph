/**
 * @author Pallavi Chavan pallucha21 and evanhowe03
 * @version 1.0
 */
public class GraphNode {

    private String key;
    private SinglyLinkedList<Integer> value;
   // Key - string artist/song
    //Value - linked lists of the songs/artsits
    //index - where it is in the adjacency list
    
    /**
     * graph node class
     */
    public GraphNode() {
        key = null;
        value = new SinglyLinkedList<Integer>();
    }
    
    /**
     * get key method
     * @return artist/song
     */
    public String getKey() {
        return key;
    }

    /**
     * set key method
     * @param k artist/song
     */
    public void setKey(String k) {
        key = k;
    }

    /**
     * get value
     * @return linked list
     */
    public SinglyLinkedList<Integer> getValue() {
        return value;
    }
    
    /**
     * set value
     * @param v linked list
     */
    public void setValue(SinglyLinkedList<Integer> v) {
        value = v;
    }

   
 
}

